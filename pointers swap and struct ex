Do some problem on pointers 

    x, y := 5,6

    Using pointers try to swap the values

    fmt.Println(x,y) 6,5
    
  package main

import "fmt"

func main() {
	var x int = 5
	var y int = 6
	a := &x
	b := &y

	fmt.Println(x, y)
	fmt.Println(&x, &y)
	fmt.Println(a, b)
	swap()
}
func swap() {

	v := swap(&a, &b)
	fmt.Println("the values changed")
	fmt.Println(v)
	fmt.Println(x, y)
}

    
    
